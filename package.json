{
  "name": "react-hacker-news",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "run-s sprite:svg _webpack:dev",
    "debug": "cross-env DEBUG_ENV=enable webpack-dev-server --mode development",
    "build": "run-s sprite:svg del:dist copy:static _webpack:production",
    "commit": "commit",
    "lint:ts": "eslint --fix \"**/*.ts{,x}\"",
    "lint:css": "stylelint \"**/*.ts{,x}\"",
    "fmt": "prettier --loglevel warn --write \"**/*.{js,ts,tsx}\"",
    "unit": "jest",
    "unit:watch": "jest --watch",
    "unit:coverage": "jest --coverage",
    "e2e": "jest --config=jest.e2e.config.js",
    "e2e:watch": "cross-env HEADLESS=false jest --config=jest.e2e.config.js",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "copy:static": "cpx \"./src/static/**/*\" \"./dist\"",
    "del:dist": "rimraf \"./dist\"",
    "sort:import": "import-sort --write \"**/*.ts{,x}\"",
    "sprite:svg": "svg-sprite-generate -d ./material/sprite -o ./src/static/img/icon/icons.svg",
    "postinstall": "rm -rf node_modules/@types/react-native",
    "_webpack:dev": "webpack-dev-server --config webpack.dev.js",
    "_webpack:production": "webpack --config webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryu-f/react-hacker-news.git"
  },
