    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.1",
    "cross-env": "^5.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rxjs": "^6.5.2",
    "styled-components": "^4.3.2",
    "typeless": "1.1.0",
    "typeless-router": "^1.1.0"
  },
