{
  "name": "react-hacker-news",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development",
    "build": "cross-env NODE_ENV=production npm-run-all del:dist copy:static webpack:production",
    "fix": "eslint --fix '**/*.ts{,x}'",
    "webpack:production": "webpack --mode production",
    "commit": "commit",
    "lint:ts": "eslint '**/*.ts{,x}'",
    "lint:css": "stylelint --fix '**/*.ts{,x}'",
    "unit": "jest",
    "unit:watch": "jest --watch",
    "unit:coverage": "jest --coverage",
    "e2e": "jest --config=jest.e2e.config.js",
    "e2e:watch": "cross-env HEADLESS=false jest --config=jest.e2e.config.js",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "copy:static": "cpx './src/static/**/*' './dist'",
    "del:dist": "rimraf './dist'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryu-f/react-hacker-news.git"
  },
  "keywords": [],
  "author": "ryu.furuta",
  "license": "ISC",
  "homepage": "https://github.com/ryu-f/react-hacker-news#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts{,x}": [
      "npm run fix",
      "npm run lint:css",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
